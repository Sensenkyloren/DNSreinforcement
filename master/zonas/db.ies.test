;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	DNSA.ies.test. root.ies.test. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	DNSA.ies.test.
@	IN	NS	DNSB.ies.test.

DNSA	IN 	A	192.168.57.10
DNSB	IN	A	192.168.57.100
